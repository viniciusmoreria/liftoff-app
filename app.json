{
  "expo": {
    "name": "Liftoff",
    "slug": "Liftoff",
    "description": "A SpaceX Tracker App",
    "primaryColor": "#05050B",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "backgroundColor": "#05050B",
    "splash": {
      "backgroundColor": "#05050B"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.viniciusmoreria.liftoff",
      "buildNumber": "1.0.0"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#05050B"
      },
      "package": "com.viniciusmoreria.liftoff",
      "versionCode": 6
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to customize your experience."
        }
      ]
    ]
  }
}
