{
  "expo": {
    "name": "Liftoff",
    "slug": "Liftoff",
    "description": "A SpaceX Tracker App",
    "privacy": "public",
    "primaryColor": "#16171B",
    "version": "1.3.4",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "backgroundColor": "#16171B",
    "splash": {
      "backgroundColor": "#16171B"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "url": "https://u.expo.dev/992860c0-c777-48bd-b67b-4f2ca4a2e6bb"
    },
    "assetBundlePatterns": ["**/*"],
    "jsEngine": "jsc",
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.viniciusmoreria.liftoff",
      "googleServicesFile": "./src/libs/firebase/GoogleService-Info.plist",
      "buildNumber": "1.3.4"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#16171B"
      },
      "package": "com.viniciusmoreria.liftoff",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.viniciusmoreria.liftoff",
      "googleServicesFile": "./src/libs/firebase/google-services.json",
      "versionCode": 440010304
    },
    "notification": {
      "icon": "./src/assets/push-icon.png",
      "color": "#16171B",
      "iosDisplayInForeground": true
    },
    "runtimeVersion": {
      "policy": "nativeVersion"
    },
    "extra": {
      "eas": {
        "projectId": "992860c0-c777-48bd-b67b-4f2ca4a2e6bb"
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to customize your experience."
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/push-icon.png",
          "color": "#16171B"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "13.0"
          }
        }
      ],
      "@react-native-firebase/app",
      "@react-native-firebase/perf",
      "sentry-expo"
    ]
  }
}
