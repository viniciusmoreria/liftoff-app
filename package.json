{
  "name": "liftoff",
  "version": "1.3.4",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": "expo start -c --android",
    "ios": "expo start -c --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "publish-staging": "expo publish",
    "publish-prod": "expo publish --release-channel production",
    "lint": "eslint --ext '.js,.ts,.tsx' --fix src",
    "format": "yarn prettier --write --list-different '**/*.{ts,tsx}'",
    "check-types": "yarn tsc -p tsconfig.json --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@expo/vector-icons": "^13.0.0",
    "@react-native-masked-view/masked-view": "0.2.6",
    "@react-navigation/material-top-tabs": "^6.2.1",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@react-navigation/stack": "^6.2.1",
    "@sentry/react-native": "^3.4.3",
    "axios": "^0.27.2",
    "expo": "~45.0.0",
    "expo-application": "~4.1.0",
    "expo-blur": "~11.1.0",
    "expo-constants": "~13.1.1",
    "expo-dev-client": "~1.0.0",
    "expo-device": "~4.2.0",
    "expo-image-picker": "~13.1.1",
    "expo-notifications": "~0.15.2",
    "expo-status-bar": "~1.3.0",
    "expo-updates": "~0.13.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-gesture-handler": "~2.2.1",
    "react-native-pager-view": "5.4.15",
    "react-native-reanimated": "~2.8.0",
    "react-native-safe-area-context": "4.2.4",
    "react-native-screens": "~3.11.1",
    "react-native-tab-view": "^3.1.1",
    "react-native-web": "0.17.7",
    "sentry-expo": "^4.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/react-native": "~0.66.13",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.1",
    "prettier": "^2.6.2",
    "standard-version": "^9.5.0",
    "standard-version-expo": "^1.0.3",
    "typescript": "~4.3.5"
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "yarn lint",
      "yarn format",
      "bash -c 'yarn check-types'"
    ]
  },
  "private": true
}
